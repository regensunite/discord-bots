{
  "name": "perspect1ve/regens-unite",
  "timeout": 10000,
  "connector": false,
  "events": {
    "functions/events/discord/command/list-people.js": {
      "name": "discord.command",
      "subtype": {
        "command": "list-people"
      }
    },
    "functions/events/discord/command/test-channels.js": {
      "name": "discord.command",
      "subtype": {
        "command": "test-channels"
      }
    },
    "functions/events/discord/command/set-bot-status.js": {
      "name": "discord.command",
      "subtype": {
        "command": "set-bot-status"
      }
    },
    "functions/events/discord/command/clear-bot-status.js": {
      "name": "discord.command",
      "subtype": {
        "command": "clear-bot-status"
      }
    },
    "functions/events/discord/message/create/prefix/rubinstall.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-install"
      }
    },
    "functions/events/discord/message/create/prefix/rubcovenant.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-covenant"
      }
    },
    "functions/events/discord/message/create/prefix/rubsudo.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-sudo"
      }
    },
    "functions/events/discord/message/create/prefix/rubarchive.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-archive"
      }
    },
    "functions/events/discord/message/create/prefix/rubgetinvolved.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-get-involved"
      }
    },
    "functions/events/discord/message/create/prefix/rubcircles.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-circles"
      }
    },
    "functions/events/discord/message/create/prefix/rubaffinities.js": {
      "name": "discord.message.create.prefix",
      "subtype": {
        "prefix": "!rub-affinities"
      }
    },
    "functions/events/discord/guild/member/add.js": {
      "name": "discord.guild.member.add"
    },
    "functions/events/discord/dm/create": {
      "name": "discord.dm.create"
    }
  },
  "env": [
    {
      "name": "REPORT_BOT_DM__CHANNEL_ID",
      "description": "the id of the channel (inside the configured Discord server) that should be used to log messages sent to the discord bot"
    },
    {
      "name": "COVENANT__CHANNEL_ID",
      "description": "the id of the channel that contains our convenant"
    },
    {
      "name": "INTRODUCE_YOURSELF__CHANNEL_ID",
      "description": "the id of the channel where members should introduce themselves"
    },
    {
      "name": "INFO_BOOTH__CHANNEL_ID",
      "description": "the id of the channel with important info, official links, etc."
    }
  ]
}
